{
  "name": "balancebike_school",
  "version": "1.0.0",
  "private": true,
  "description": "Лендинг для беговелошколы Беговеломания",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "7.2.x",
    "browser-sync": "^2.24.6",
    "del": "^3.0.0",
    "gh-pages": "^1.2.0",
    "gulp": "3.9.x",
    "gulp-cssmin": "^0.2.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-jsmin": "^0.1.5",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-rename": "^1.4.0",
    "gulp-webp": "^3.0.0",
    "run-sequence": "^2.2.1",
    "stylelint": "8.4.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.9"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  }
}
